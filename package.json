{
  "name": "fausto-web2020",
  "version": "7.2.2",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run FaustoWeb2020:serve-ssr",
