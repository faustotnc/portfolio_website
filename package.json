    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/platform-server": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@angular/service-worker": "~13.1.1",
    "@nguniversal/express-engine": "^13.0.1",
    "express": "^4.15.2",
    "gsap": "^3.9.1",
    "rxjs": "~6.5.4",
    "three": "^0.136.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
