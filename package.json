{
  "name": "fausto-web2020",
  "version": "7.2.2",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run FaustoWeb2020:serve-ssr",
    "serve:ssr": "node dist/FaustoWeb2020/server/main.js",
    "build:ssr": "ng build --configuration production --named-chunks true --optimization true --aot true --build-optimizer true --common-chunk true && ng run FaustoWeb2020:server:production",
    "prerender": "ng run FaustoWeb2020:prerender",
    "deploy:gcp": "npm run build:ssr && gcloud app deploy"
  },
  "private": true,
